{
    "name": "kdl-app",
    "version": "1.40.0",
    "private": true,
    "release": {
        "tagFormat": "kdl-app-v${version}",
        "extends": "semantic-release-monorepo",
        "branches": [
            "main",
            {
                "name": "develop",
                "prerelease": "rc"
            }
        ],
        "plugins": [
            "@semantic-release/commit-analyzer",
            "@semantic-release/release-notes-generator"
        ]
    }
}
