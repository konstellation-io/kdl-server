{
  "extends": "../tsconfig.json",
  "include": [
    "*/*.ts"
  ],
  "exclude": [
    "node_modules/*/*.*", "**/*.spec.js", "**/*.spec.ts", "**/*.spec.tsx", "**/*.d.ts", "cypress/*"
  ],
  "types":["node"],
  "jsx": "react-jsx",
  "moduleResolution": "node",
  "compilerOptions": {
    "noEmit": false,
    "isolatedModules": false,
    "types": ["cypress", "node"],
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
  },
}
