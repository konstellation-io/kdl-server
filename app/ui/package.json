{
  "name": "konstellation-dev-lab",
  "version": "v1.22.5",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:cov": "react-scripts test --coverage --watchAll=false",
    "test:cov:sonar": "react-scripts test --silent --env=jsdom --coverage --watchAll=false --testResultsProcessor jest-sonar-reporter",
    "eject": "react-scripts eject",
    "prettier:types": "prettier --write --single-quote \"src/Graphql/**/types/**/*.ts\"",
    "gen:types": "apollo client:codegen types --globalTypesFile=src/Graphql/types/globalTypes.ts --target typescript --includes=./src/Graphql/\"{queries,mutations,fragments}\"/*.ts --localSchemaFile=../api/infrastructure/graph/schema.graphqls && yarn prettier:types",
    "cy": "cypress open",
    "cy:run": "cypress run --headless --browser chrome",
    "lint": "eslint src cypress",
    "lint:fix": "eslint src cypress --fix"
  },
  "dependencies": {
    "@apollo/client": "3.5.5",
    "classnames": "2.3.1",
    "graphql": "15.5.0",
    "is-electron": "2.2.1",
    "kwc": "1.15.1",
    "lodash.capitalize": "4.2.1",
    "moment-timezone": "0.5.33",
    "node-sass": "6.0.1",
    "react": "17.0.2",
    "react-animate-height": "2.0.23",
    "react-dom": "17.0.2",
    "react-gravatar": "2.6.3",
    "react-hook-form": "6.14.2",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3",
    "react-table": "7.7.0",
    "react-tabs": "3.2.3",
    "react-toastify": "7.0.4",
    "react-transition-group": "4.4.2",
    "typeface-montserrat": "1.1.13",
    "typeface-roboto-mono": "1.1.13",
    "typescript": "4.5.2"
  },
  "devDependencies": {
    "@bahmutov/add-typescript-to-cypress": "2.1.2",
    "@testing-library/jest-dom": "5.12.0",
    "@testing-library/react": "11.2.6",
    "@testing-library/user-event": "13.1.9",
    "@types/classnames": "2.3.1",
    "@types/enzyme": "3.10.10",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/jest": "26.0.23",
    "@types/lodash.capitalize": "4.2.6",
    "@types/node": "15.0.3",
    "@types/react": "17.0.37",
    "@types/react-dom": "17.0.11",
    "@types/react-gravatar": "2.6.10",
    "@types/react-router-dom": "5.1.7",
    "@types/react-table": "7.7.8",
    "@types/react-tabs": "2.3.3",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.1",
    "apollo": "2.33.9",
    "cypress": "8.7.0",
    "enzyme": "3.11.0",
    "enzyme-to-json": "^3.6.2",
    "eslint": "8.3.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.27.1",
    "eslint-plugin-react-hooks": "4.3.0",
    "husky": "6.0.0",
    "jest-canvas-mock": "2.3.1",
    "jest-sonar-reporter": "^2.0.0",
    "lint-staged": "11.0.0",
    "mock-apollo-client": "1.2.0",
    "mutationobserver-shim": "^0.3.7",
    "prettier": "2.3.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/**/*.tsx",
      "src/**/*.ts",
      "!src/index.tsx",
      "!Graphql",
      "Graphql/hooks/**/*.ts",
      "!types/**/*.ts",
      "!browserHistory.ts",
      "!fetchConfig.ts",
      "!react-app-env.d.ts",
      "!src/Pages/Project/pages/KG/components/KGVisualization"
    ],
    "coverageReporters": [
      "text",
      "lcov"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
